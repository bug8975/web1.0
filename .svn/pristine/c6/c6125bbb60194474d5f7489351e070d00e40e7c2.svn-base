package com.monitor.core.tools;

import java.util.Date;

import com.monitor.core.constant.Globals;

public class Logger {
    public static void printlnWithTime(String string){
    	StringBuffer sb = new StringBuffer();
    	sb.append(Globals.DateFormatWITHMS.format(new Date()));
    	sb.append(" ");
    	sb.append(string);
    	System.out
    		.println(sb.toString());
    }
}
