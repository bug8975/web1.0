.indexmiddle{float:left;height:388px; background-color:#F00;width:702px;}
.index .navright ul{ border-bottom:0;}
.index .index{ min-height:388px;}
/*轮播图 star*/
#divSmallBox{overflow:hidden;*display:inline;*zoom:1;width:10px;height:10px;margin:0 5px;border-radius:10px;background:#ffffff;}
#playBox{ width:100%; height:388px; background:#333; position:relative; overflow:hidden;}
#playBox .oUlplay { width:99999px; position:absolute; left:0; top:0;}
#playBox .oUlplay li{ float:left; height:388px;}
#playBox .oUlplay li img{ width:100%;}
#playBox .pre{ cursor:pointer; width:45px; height:45px; background:url(../images/l.png) no-repeat; position:absolute; top:50%; left:10px; z-index:10; margin-top:-23px}
#playBox .next{ cursor:pointer; width:45px; height:45px; background:url(../images/r.png) no-repeat; position:absolute; top:190px; right:10px; z-index:10;}
#playBox .smalltitle {width:100%; height:10px; position:absolute; bottom:15px; z-index:10}
#playBox .smalltitle ul{ width:132px; margin:0 auto;}
#playBox .smalltitle ul li{ width:10px; height:10px; margin:0 5px; border-radius:100%; background:#302f31; border:1px solid #302f31;float:left; overflow:hidden;*display:inline;*zoom:1;}
#playBox .smalltitle .thistitle{ background:#e2dbe9;}
/*轮播图 end*/

.indexright{ float:right; overflow:hidden;width:316px;}
.recomright li.wh1000{ display:block}
.indexright .title{ font-size:16px; color:#fff; height:44px; line-height:44px; position: relative;padding-left:16px; top:1px;}
.indexright .contain{ height:234px; overflow:hidden; background-color:#fff;}
.indexright .title i{ display:block; position:absolute; background-color:#fff; width:18px; height:18px; line-height:18px; text-align:center; color:#4c9cff; left:98px; top:14px; font-weight:bold;}
.indexright .title i a{ color:#4c9cff}
.indexright .contain ul{ border:1px solid #e4e4e4;}
.indexright .contain ul li{ padding:0 16px; height:38px; line-height:38px; border-bottom:1px solid #e4e4e4; overflow:hidden;text-overflow:ellipsis}
.indexright .contain ul li .time{ color:#2674d5; font-size:14px; margin-right:14px;}
.indexright .contain ul li a{ color:#5f5f5f; font-size:14px;}
.indexright .contain ul li a:hover{color:#2674d5;}
