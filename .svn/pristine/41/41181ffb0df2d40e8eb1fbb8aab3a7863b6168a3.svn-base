.floor{ margin-bottom:36px; width:1202px;}
.floorleft{ width:182px; float:left; position:relative}
.floorleft .title{ height:44px; line-height:44px; font-size:21px; color:#2d2d2d; border-bottom:4px solid #4c9cff}
.floorleft .title i{ padding:2px; background-color:#4c9cff; color:#fff; font-size:21px; margin-right:22px;}
.floorleft .contain{ width:158px; height:424px; background-color:#487dd9; padding:10px 12px 0 12px;}
.floorleft .contain ul li{ height:32px; line-height:32px; float:left; min-width:79px; max-width:158px; overflow:hidden;text-overflow:ellipsis; color:#fff;}
.floorleft .contain ul li a{ font-size:16px; color:#fff; border-bottom:1px dashed #fff; font-weight:bold}
.floorleft .contain .img_botm{ position:absolute; bottom:0px; right:0; width:100px; }
.floorright{ float:left; font-size:16px; color:#626262; position:relative;}
.floorright .notice { width:1016px;}
.floorright .notice .tab-hd {color: #404040; position:relative }
.floorright .notice .tab-hd ul { height:44px; line-height:44px; border-bottom:4px solid #4c9cff}
.floorright .notice .tab-hd li{ float:left;}
.floorright .notice .tab-hd span{ float:left; margin:0 8px; color:#e4e4e4;}
.floorright .notice .tab-hd li a{ display:block; font-size:16px; color:#626262; height:38px; padding:0 22px;}
.floorright .notice .tab-hd li a:hover{ }
.floorright .notice .tab-hd li.on a{ color:#2981ee; border-bottom:1px solid #4c9cff; text-decoration:none;}
.floorright .notice .tab-pal{width:1015px; border:1px solid #e4e4e4; border-left:0; background-color:#f0f0f0}
.floorright .notice .tab-pal ul{ width:1016px; padding-bottom:10px; height:422px;}
.floorright .notice .tab-pal li{ width:189px;float:left; border:1px solid #e4e4e4; background-color:#fff; margin:10px 0 0 10px; padding-top:10px; box-shadow:1px 1px 4px #ddd }
.floorright .notice .tab-pal li .img{width:180px; height:120px; overflow:hidden; margin:0 auto; padding-bottom:10px}
.floorright .notice .tab-pal li a img{ width:180px; height:120px; transition: all 0.8s ease 0s; }
.floorright .notice .tab-pal li:hover img {transform: scale(1.1);}
.floorright .notice .tab-pal li:hover .title{ color:#4c9cff}
.tab-hd .more{ position:absolute; right:0; top:12px; font-size:12px; color:#fff; background-color:#4c9cff; padding:2px 4px;}
.floorright .notice .tab-pal li .title{ margin-top:10px; font-size:16px; color:#626262;line-height:18px; height:38px; overflow:hidden; margin-bottom:10px; padding:0 10px}
.floorright .notice .tab-pal li .price{ color:#4c9cff; font-size:16px; text-align:center; margin-top:6px; display:none;}
.floorright .notice .tab-pal li .price span{ font-size:26px;}
.floor_recomm{ width:100%; height:86px; clear:both; }
.floor_recomm_l{ height:86px; width:182px; background-color:#2d2d2d; line-height:86px; text-align:center; font-size:20px; color:#fff; float:left;}
.floor_recomm_r{float:left; height:86px;/* background-color:#f5f5f5;*/ width:1004px; font-size:40px; line-height:86px; padding-left:10px; overflow:hidden; text-align:center; border:1px solid #e4e4e4; border-top:0;}
.floor_recomm_r a{ display:inline-block; float:left; margin-top:8px; width:166px; text-align:center; border-right:1px dashed #cccccc; opacity:0.8;filter:alpha(opacit=80)}
.floor_recomm_r a:hover{ opacity:1;filter:alpha(opacity=100)}
.floor_recomm_r a img{ width:100px; height:67px;}
.floor_recomm_r a.last{border-right:0;}

.cooperation{ width:100%; height:162px;}
.cooperation .title{ height:44px; border-bottom:4px solid #4c9cff; color:#2d2d2d; font-size:21px; padding-left:50px; position:relative;}
.cooperation .title h4{ font-weight:normal; line-height:44px;}
.cooperation .title .more{position:absolute; right:0; top:12px; font-size:12px; color:#fff; background-color:#4c9cff; padding:2px 4px;}
.cooperation .contain{ margin-top:10px; width:1199px; border-left:1px solid #e5e5e5; border-right:1px solid #e5e5e5; line-height:104px; height:104px; overflow:hidden;}
.cooperation .contain ul{ width:1200px;}
.cooperation .contain ul li{ border:1px solid #e5e5e5; border-left:0; width:149px; height:102px; float:left;}
.cooperation .contain ul li img{ width:149px; height:102px;} 
.cooperation .contain ul li:hover img{width:147px; height:100px; border:1px solid #4c9cff}
.banner{ width:100%; margin-bottom:50px; height:116px; overflow:hidden;}
.banner img{ width:100%; display:block; transition: all 0.8s ease 0s;}
.banner:hover img{transform: scale(1.2); }

