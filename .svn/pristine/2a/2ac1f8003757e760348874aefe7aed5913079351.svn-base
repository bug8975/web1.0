/**********公共头部************/
.top{ width:100%; height:35px; line-height:35px; border-bottom:1px solid #d3d3d3;}
.page{ margin:0 auto; height:100px; line-height:100px; width:100%;background-color:#656565}
.pageleft{ float:left;font-size:14px;}
.welcome{ margin-right:35px;}
.pxlm{ margin:0 15px;}
.lightblue{ color:#5c5c5c}
.pageright{ float:right; font-size:14px;}
.pageright .quick-menu{height:35px; line-height:35px;}
.pageright .quick-menu li{ float:left; color:#e4e4e4; font-size:16px; line-height:35px; }
.pageright .quick-menu li .menu{ position:relative; z-index:1000}
.pageright .quick-menu li .menu .menu-bd{ position:absolute; top:35px; left:0; width:82px; z-index:10; background-color:#fff; border:1px solid #4c9cff; display:none; padding:6px 0;}
.pageright .quick-menu li .menu .menu-bd a:hover { color:#4c9cff}
.pageright .quick-menu li a{ color:#5c5c5c;display:inline-block; padding:0 8px; font-size:14px; }
.pageright .quick-menu .menu-bd-panel a{ display:block; line-height:20px; height:20px;}
.pageright .quick-menu li a:hover{color:#477edb}
.pageright .quick-menu .menu-bd{ display:block}
