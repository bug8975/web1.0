/**********公共底部************/
.footer{width:100%;}
.footer_up{ width:100%; padding:10px 0 20px 0; background-color:#487dd9}
.floor .floor_box{ width:1202px;}
.footer_upinfo{ width:1200px; margin:0 auto;}
.shop{ padding:10px 36px 20px 34px; float:left; width:130px; box-sizing: content-box; -webkit-box-sizing: content-box;}
.shop .title{ height:34px; line-height:34px; border-bottom:1px solid #7ba1e4; font-size:12px; color:#b6cbf0}
