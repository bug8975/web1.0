.main_bj{ width:100%; height:390px; background:repeat-x 0 0; position:absolute; left:0; top:214px;}
.main{width:100%; margin:0 auto; position:relative; min-height:540px}
.main .header{ width:100%; padding-bottom:20px;}
/*.main .header{ width:100%; height:74px;padding:30px 0; padding-top:30px; padding-bottom:32px;}*/
.logo{ width:196px; height:44px; float:left; margin-right:66px; position:relative;top:-15px;}
.toph_bigsearch{ float:left; font-size:14px; color:#959595; font-size:0; position:relative; width:578px;}
.toph_sear{ width:100%; height:42px; line-height:42px; border:2px solid #4c9cff; position:relative;}
.toph_sear .seek{ width:106px;font-size:14px; border:1px solid #d4d4d4; background-color:#f5f5f5; text-align:center; float:left; border-bottom:0; line-height:40px; }
.toph_sear .seek_list{ background-color:#fff; border:1px solid #d4d4d4; padding:4px 0 0 0;width:106px; position:relative; left:-1px; display:none;z-index:2}
.toph_sear .seek_list li{ height:26px; line-height:26px; cursor:pointer; text-align:center; }
.toph_sear .seek_list li:hover{ background-color:#f5f5f5; color:#4c9cff}
.toph_sear .seek_list li:hover a{ color:#4c9cff}
.toph_sear .seek a{ color:#959595;}
.toph_sear .seek a i{ color:#4c9cff}
.toph_sear .search{ float:left; border:0; height:40px; line-height:40px; padding-left:12px; width:412px; font-size:14px;}
.search_btn{ position:absolute; width:48px; background-color:#4b9efe; right:0; top:0; height:44px; line-height:36px; text-align:center;color:#fff;}
.search_btn:hover{ color:#fff}
.search_btn i{font-size:30px; }
.keyword{ padding-top:12px; font-size:12px; position:absolute; top:46px; left:0; z-index:1; font-size:14px;}
.keyword a{color:#959595; margin:0 6px;} 
.keyword a:hover{color:#477edb; text-decoration: underline}
.stay_big{ float:right; overflow:hidden}
.stay{ display:block; width:120px; height:44px; line-height:44px;color:#fff; font-size:16px; line-height:40px; background-color:#4c9cff;margin-left:10px; float:left; text-align:center}
.stay i{ margin-right:4px; font-size:20px;}
.stay:hover{ color:#fff;}

.stay_in{ display:block; width:138px; height:44px; line-height:44px;color:#fff; font-size:16px; line-height:40px; background-color:#4c9cff;margin-left:10px; float:left; text-align:center;}
.stay_in i{ margin-right:4px; font-size:20px;}
.stay_in:hover{ color:#fff;}
.nav{ width:100%; height:44px; line-height:44px; font-size:0;}
.navleft{ font-size:18px; color:#fff; background-color:#4c9cff; text-align:center; width:182px; float:left; position:relative; font-weight:bold; }
.navright ul{ width:100%; height:42px;/* border-bottom:2px solid #4c9cff*/}
.navright ul li{ float:left; height:42px; line-height:44px; margin:0 16px; min-width:66px; text-align:center}
.navright ul li a{ font-size:16px; color:#3b3b3b;}
.navright ul li a:hover{color:#4c9cff;font-weight:bold;}
.navright ul li.current a{ color:#4c9cff; font-weight:bold;}

.index{ width:100%; min-height:486px;}
.indexleft{width:182px; height:390px; background-color:#4c9cff; float:left; font-size:0; margin-top:-2px; position:relative; text-align:left;}
.indexleft ul li{border-top:1px solid #e4e4e4; padding:10px 0 10px 8px; position:relative;}
.indexleft ul li .title{ font-size:16px; line-height:34px; color:#fff; }
.indexleft ul li .title i{ color:#fff; margin-right:10px;font-size:16px; }
.indexleft ul li .title a{ color:#fff; font-weight:bold;}
.indexleft ul li.menulihover .title a{ color:#000;}
.indexleft ul li.menulihover .title i{ color:#4c9cff}
.indexleft ul li .contain{ line-height:20px; padding-right:10px; height:42px; overflow:hidden}
.indexleft ul li .contain a{ font-size:14px; color:#fff; margin-right:14px; display:block; float: left; font-weight:normal}
.indexleft ul li .contain a:hover{ color:#4c9cff;}
.indexleft ul li .right_icon{ position:absolute; width:20px; height:20px; top:50%; margin-top:-10px;right:0; line-height:20px; text-align:center; }
.indexleft ul li.menulihover{ background-color:#f3f3f3}
.indexleft ul li.menulihover .title{ color:#2d2d2d;}
.indexleft ul li.menulihover .contain a{ color:#646464;}
.indexleft ul li.menulihover .right_icon i{ color:#4c9cff}
.yMenuListConin{position:absolute; top:0; left:182px; z-index:20;}
.yMenuListCon .list2{ top:97px;}
.yMenuListCon .list3{ top:195px;}
.yMenuListCon .list4{ top:293px;}
.limore{ width:272px; background-color:#f3f3f3;/* height:563px; */position:absolute; top:0; left:182px; z-index:20; padding:14px 32px 20px 32px; display:none; /*min-height:356px;*/}
.limore ul li{ border-bottom:1px dashed #cacaca; height:32px; line-height:32px; overflow:hidden; padding:0;margin-right:6%; float:left;}
.limore ul li a{ font-size:14px; color:#2d2d2d; font-weight:normal}
.limore ul li{ border-bottom:1px dashed #cacaca; height:32px; line-height:32px; overflow:hidden; padding:0; border-top:0}
.limore ul li a:hover{ color:#4c9cff}
.indexleft ul li .right_icon i{ font-size:12px; color:#fff}
.indexleft ul li.last{ border-bottom:0;}

